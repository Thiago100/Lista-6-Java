import java.util.*;
import java.io.*;
public class Main {
	public static void main(String args[]){
		Scanner sc = new Scanner(System.in);
		Scanner scanner = new Scanner(System.in);
		PrintStream sa = System.out;
		String a = sc.next();
		int p=0;
		for(int i=0;i<a.length();i++){
            if(a.charAt(i)=='1') p++;
		}
		if(p%2==0) sa.println(a+"0");
		else sa.println(a+"1");

	}
 }
